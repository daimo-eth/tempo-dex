{
  "name": "tempo-dex-ui",
  "license": "GPL-3.0-or-later",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "esbuild src/app.tsx --bundle --outfile=public/dist/bundle.js --sourcemap --servedir=public --define:__INDEX_SUPPLY_API_KEY__=\\\"${INDEX_SUPPLY_API_KEY:-}\\\"",
    "build": "esbuild src/app.tsx --bundle --outfile=public/dist/bundle.js --minify --define:__INDEX_SUPPLY_API_KEY__=\\\"${INDEX_SUPPLY_API_KEY:-}\\\"",
    "start": "serve public -l ${PORT:-8080}",
    "typecheck": "tsc --noEmit",
    "test": "tsc --noEmit && node --import tsx --test src/*.test.ts"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.14",
    "connectkit": "^1.9.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "serve": "^14.2.4",
    "tempo.ts": "^0.12.1",
    "viem": "^2.43.3",
    "wagmi": "^2.19.5"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "esbuild": "^0.23.1",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.6.2",
    "vitest": "^4.0.16"
  }
}
